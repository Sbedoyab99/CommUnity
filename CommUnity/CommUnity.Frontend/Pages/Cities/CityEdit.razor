<MudDialog>
    <TitleContent>
        <MudText Typo="Typo.h6">Editar Ciudad</MudText>
    </TitleContent>
    <DialogContent>
        @if (city is null)
        {
            <Loading />
        }
        else
        {
            <FormWithName @ref="cityForm" Label="Ciudad:" Model="city" OnValidSubmit="SaveAsync" ReturnAction="Return" />
        }
    </DialogContent>
</MudDialog>
