<MudDialog>
    <TitleContent>
        <MudText Typo="Typo.h6">Editar Unidad Residencial</MudText>
    </TitleContent>
    <DialogContent>
        @if (loading)
        {
            <Loading />
        }
        else
        {
            <ResidentialUnitFormWithCity @ref="residentialUnitFormWithCity" ResidentialUnit="residentialUnit" OnValidSubmit="EditResidentialUnitAsync" ReturnAction="Return" CityId="CityId" />
        }
    </DialogContent>
</MudDialog>