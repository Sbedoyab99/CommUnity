<MudDialog>
    <TitleContent>
        <MudText Typo="Typo.h6">Editar Mascota</MudText>
    </TitleContent>
    <DialogContent>
        @if (pet is null)
        {
            <Loading />
        }
        else
        {
            <PetForm @ref="petForm" Pet="pet" OnValidSubmit="EditAsync" ReturnAction="Return" />
        }
    </DialogContent>
</MudDialog>



