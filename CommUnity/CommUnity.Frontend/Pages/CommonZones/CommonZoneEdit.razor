<MudDialog>
    <TitleContent>
        <MudText Typo="Typo.h6">Editar Zona Común</MudText>
    </TitleContent>
    <DialogContent>
        @if (commonZone is null)
        {
            <Loading />
        }
        else
        {
            <CommonZoneForm @ref="commonZoneForm" CommonZone="commonZone" OnValidSubmit="EditAsync" ReturnAction="Return" />
        }
    </DialogContent>
</MudDialog>


