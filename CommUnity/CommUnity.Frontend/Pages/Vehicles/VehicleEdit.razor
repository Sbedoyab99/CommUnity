<MudDialog>
    <TitleContent>
        <MudText Typo="Typo.h6">Editar Vehículo</MudText>
    </TitleContent>
    <DialogContent>
        @if (vehicle is null)
        {
            <Loading />
        }
        else
        {
            <VehicleForm @ref="vehicleForm" Vehicle="vehicle" OnValidSubmit="EditAsync" ReturnAction="Return" />
        }
    </DialogContent>
</MudDialog>



