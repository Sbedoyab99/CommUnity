<MudDialog>
    <TitleContent>
        <MudText Typo="Typo.h6">Editar Pais</MudText>
    </TitleContent>
    <DialogContent>
        @if (country is null)
        {
            <Loading />
        }
        else
        {
            <FormWithName @ref="countryForm" Label="Pais: " Model="country" OnValidSubmit="EditAsync" ReturnAction="Return" />
        }
    </DialogContent>
</MudDialog>


